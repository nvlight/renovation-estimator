        "vue-router": "^4.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@pinia/testing": "^1.0.0",
        "@quasar/app-vite": "^2.1.0",
        "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
        "@vitest/ui": "^2.0.5",
        "@vue/eslint-config-prettier": "^10.1.0",
        "@vue/test-utils": "^2.4.4",
        "autoprefixer": "^10.4.2",
        "eslint": "^9.14.0",
        "eslint-plugin-vue": "^9.30.0",
        "globals": "^15.12.0",
        "postcss": "^8.4.14",
        "prettier": "^3.3.3",
        "vite-plugin-checker": "^0.9.0",
        "vitest": "^2.0.5"
    },
    "engines": {
        "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
